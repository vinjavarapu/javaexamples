package collectionpractice;

import java.util.ArrayList;

public class arraylistclass {
	
	public static void main(String[] args) {
		
		ArrayList arrlist = new ArrayList();
		arrlist.add("Mango");
		arrlist.add("Apple");
		arrlist.add("Banana");
		System.out.println(arrlist);
		arrlist.add("Straberry");
		System.out.println(arrlist);
	}

}
